<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="AppBundle\Entity\User" table="user">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="mail" type="string" column="mail" length="255" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="pass" type="string" column="pass" length="64" nullable="false">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="imageurl" type="text" column="imageUrl" length="65535" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="dateregister" type="datetime" column="dateRegister" nullable="true"/>
    <field name="datelastlogon" type="datetime" column="dateLastLogon" nullable="true"/>
    <field name="pesel" type="string" column="pesel" length="11" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
    <field name="approleid" type="integer" column="appRoleId" nullable="false">
      <options>
        <option name="unsigned"/>
      </options>
    </field>
    <field name="status" type="boolean" column="status" nullable="true"/>
  </entity>
</doctrine-mapping>
